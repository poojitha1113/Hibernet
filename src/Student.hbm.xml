<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
        <class name="com.Hcm.Student" table="EmployeeData">
            <id name="id" column="Eid"></id>
            <property name="name" column="FirstName"></property>
            <property name="adrs" column="LastName"></property>
            <property name="lName" column=""></property>
            <property name="eSalary" column="ESalary"></property>
        </class>
</hibernate-mapping>