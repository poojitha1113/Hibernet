<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
        <class name="com.basic.Employee" table="EmployeeData">
            <id name="id" column="Eid"></id>
            <property name="fname" column="FirstName"></property>
            <property name="lName" column="LastName"></property>
            <property name="address" column="EAddress"></property>
            <property name="salary" column="ESalary"></property>
        </class>
</hibernate-mapping>